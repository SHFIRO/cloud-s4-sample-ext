										<Label text="{i18n>accountNumber}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
									</form:label>
									<form:fields>
										<Text text="{BankAccount}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
									</form:fields>
								</form:FormElement>
								<form:FormElement visible="true">
									<form:label>
										<Label text="{i18n>accountHolderName}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
									</form:label>
									<form:fields>
										<Text text="{BankAccountHolderName}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
									</form:fields>
								</form:FormElement>
								<form:FormElement visible="true">
									<form:label>
										<Label text="{i18n>SWIFTCode}" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
									</form:label>
									<form:fields>
										<Text text="{SWIFTCode}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
									</form:fields>
								</form:FormElement>
								<form:FormElement visible="true">
